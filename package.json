{
  "name": "react-ssr-hydration",
  "version": "1.0.0",
  "description": "React SSR",
  "main": "index.js",
  "engines": {
    "node": "10.x"
  },
  "scripts": {
    "prod": "npm run cpFiles && webpack --env.NODE_ENV=production && node ./dist/server.js",
    "dev-all": "npm run cpFiles && webpack --env.NODE_ENV=development && concurrently \"webpack --env.NODE_ENV=development --watch\" \"nodemon ./dist/server.js\"",
    "lint": "./node_modules/.bin/eslint . && echo 'Lint successful and no errors found'",
    "jest": "node_modules/.bin/jest",
    "jest-coverage": "node_modules/.bin/jest --coverage",
    "build": "webpack --env.NODE_ENV=production --env.HOST=0.0.0.0",
    "start": "node ./dist/server.js",
    "cpFiles": "cp ./sw.js ./dist/sw.js && cp ./manifest.json ./dist/manifest.json && cp ./robots.txt ./dist/robots.txt"
  },
  "author": "Atul Anand",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-runtime": "^6.26.0",
    "eslint": "^7.2.0",
    "eslint-plugin-react": "^7.20.0",
    "json-loader": "^0.5.4",
    "webpack-dev-server": "^3.11.0",
    "prop-types": "^15.6.1",
    "jest": "^23.6.0",
    "concurrently": "^5.2.0"
  },
  "dependencies": {
    "@atlaskit/button": "^8.0.1",
    "@atlaskit/checkbox": "^3.0.3",
    "axios": "^0.19.2",
    "express": "^4.14.0",
    "fetch-mock": "^9.10.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "react-vis": "^1.11.7",
    "redux": "^4.0.5",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^2.3.0",
    "webpack-node-externals": "^1.2.0",
    "css-loader": "^3.6.0",
    "node-sass": "^7.0.0",
    "sass-loader": "^7.3.1",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "bulma": "^0.9.0",
    "babel-core": "^6.10.4",
    "compression": "^1.0.3"
  }
}
